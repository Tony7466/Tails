msgid "[[!meta title=\"Tails 4.10 is out\"]]\n"
msgstr "[[!meta title=\"Tails 4.10\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Tue, 25 Aug 2020 12:34:56 +0000\"]]\n"
msgstr "[[!meta date=\"Tue, 25 Aug 2020 12:34:56 +0000\"]]\n"

